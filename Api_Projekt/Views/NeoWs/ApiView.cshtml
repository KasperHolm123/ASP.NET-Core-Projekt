@model Api_Projekt.Models.Asteroid

@{
    ViewData["Title"] = "Nasa Response";
}

<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-info">Nasa's Asteroids</h2>
    </div>
    <div class="col-3 offset-3">
        <a asp-action="Index" asp-controller="NeoWs" class="btn btn-danger form-control text-white">
            Back
        </a>
    </div>
    <form method="post">
        <div class="col-12 border p-3">
            <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Hazardous</th>
                        <th>Close Approach Date</th>
                        <th></th>
                    </tr>
                </thead>
            </table>
        </div>
    </form>
</div>

@section Scripts{
    <script src="~/js/NeoWs.js"></script>
}